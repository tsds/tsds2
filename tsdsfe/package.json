{
	"name": "tsdsfe",
  	"version": "0.1.2",
  	"engines":{"node": "0.12.x"},
	"dependencies": {
		"request" : "2.9.203",
		"express" : "3.0.0",
		"xml2js"  : "*",
		"cli-color": "*",
		"mkdirp": "*",
		"yargs": "*",
		"tsdset": "https://github.com/rweigel/tsdset/tarball/master",
		"datacache": "https://github.com/rweigel/datacache/tarball/master",
		"viviz": "https://github.com/rweigel/viviz/tarball/master",
		"treeify": "https://github.com/rweigel/treeify/tarball/master"
	},
	"scripts": {
		"start": "node tsdsfe.js",
		"debug1": "nodemon tsdsfe.js --debugtoconsole all --checkdeps false --checkservers false",
		"debug2": "nodemon tsdsfe.js --debugtoconsole all --startdeps",
		"preinstall": "cd deps; make bin/pngquant; cd ../../autoplot/; make install; make stop; make start",
		"test": "bash test/test.sh  2>&1 | tee test/test.log"
	}
}