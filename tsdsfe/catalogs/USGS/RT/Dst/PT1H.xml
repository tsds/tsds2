<catalog xmlns:xlink="http://www.w3.org/1999/xlink" id="USGS/RT/Dst/PT1H"
	name="Real Time Dst from USGS; 1-hour cadence.">
	<documentation xlink:href="http://magweb.cr.usgs.gov/data/indices/beta/Dst_hour/"
		xlink:title="Catalog derived from inspection of" />

	<dataset name="Real-time Dst" id="Dst" label="Dst" urltemplate="http://magweb.cr.usgs.gov/data/indices/beta/Dst_hour/usgs$Y$m$ddst4.hour"
		timeformat="$Y-$m-$d $H:$M:$S" timecolumns="1,2"
		datareader="lasp.tss.iosp.ColumnarAsciiReader">
		<documentation xmlns:xlink="http://www.w3.org/1999/xlink"
			xlink:href="http://geomag.usgs.gov/products/" xlink:title="Real-time Dst estimate from USGS; 1-hour cadence." />
		<timeCoverage>
			<Start>2013-01-03</Start>
			<End>PT0D</End>
		</timeCoverage>
		<variables>
			<variable id="DOY" name="Day of Year" label="Day of Year"
				units="" type="scalar" fillvalue="" format="%03d" columns="3" lineregex="^[0-9]" />
			<variable id="DST_RT" name=" Real-time Dst" label=" Real-time Dst"
				units="nT" type="scalar" fillvalue="" format="%.2f" columns="4"
				lineregex="^[0-9]" />
		</variables>
	</dataset>
</catalog>