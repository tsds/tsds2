<?xml version="1.0" encoding="UTF-8"?>

<catalogs>
	<catalog>
		<name>SSCWeb</name>
		<id>sscweb</id>
		<urlgenerator>http://tsds.net/catalogs/SSCWeb-urls.xsl</urlgenerator>
		<ncml>
			<generator>http://tsds.net/catalogs/SSCWeb-ncml.xsl</generator>
			<iosp>lasp.tss.iosp.ColumnarAsciiReader</iosp>
			<timecols>1,2,3</timecols>
			<timefmt>yyyy D HH:mm</timefmt>
			<fillvalue>1e31</fillvalue>
		</ncml>
		<tsdsfecatalog>http://tsds.net/catalogs/SSCWeb-tsdsfe.xml</tsdsfecatalog>
		<thredds>
			<generator>http://tsds.org/code#SSCWeb</generator>
			<refresh>PT1D</refresh>
		</thredds>
		<notes>
			<note>Some SSCWeb output options are not available through this interface at this time.</note>
		</notes>
	</catalog>
