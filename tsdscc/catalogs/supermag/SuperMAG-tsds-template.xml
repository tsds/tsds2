<catalog xmlns:xlink="http://www.w3.org/1999/xlink" id="SuperMAG" name="SuperMAG">
	<documentation xlink:href="http://supermag.jhuapl.edu/" xlink:title="SuperMAG web page"/>
	<documentation xlink:href="http://github.com/tsds/tsds2/tsdscc/" xlink:title="Catalog generation software"/>
	<documentation xlink:href="/" xlink:title="Catalog generation date: "/>
	<documentation xlink:href="https://github.com/rweigel/datacache/blob/master/plugins/supermag.js" xlink:title="DataCache plug-in"/>

	<dataset name="" ID="" timecolumns="1,2,3,4,5,6" timeformat="$Y,$m,$d,$H,$M,$S" plugin="supermag" urltemplate="http://supermag.jhuapl.edu/mag/lib/services/%3Fservice=mag%26user=tsds2%26start=$Y-$m-$dT00:00:00.000Z%26interval=23%3A59%26stations=$ID%26delta=none%26baseline=all%26format=ascii%26options=%2Bimfgsm%2Bsza">

		<documentation xlink:href="http://supermag.jhuapl.edu/info/coordinates.html" xlink:title="Coordinate system information"/>
		<documentation xlink:href="http://supermag.uib.no/info/rulesoftheroad.html/" xlink:title="Acknowledging SuperMAG's data providers"/>
		<documentation xlink:href="http://supermag.uib.no/info/acknowledgement.html/" xlink:title="Acknowledging the SuperMAG data service"/>

		<timeCoverage>
			<Start></Start>
			<End></End>
			<Cadence>PT1M</Cadence>			
		</timeCoverage>

		<variables>
			<variable id="B_N" name="B North GEO" label="B North GEO" units="nT" type="scalar" fillvalue="999999" rendering="%.1f" columns="8"/>
			<variable id="B_E" name="B East GEO" label="B East GEO" units="nT" type="scalar" fillvalue="999999" rendering="%.1f" columns="9"/>
			<variable id="B_Z" name="B Vertical GEO" label="B Vertical GEO" units="nT" type="scalar" fillvalue="999999" rendering="%.1f" columns="10"/>
			<variable id="SZA" name="Solar Zenith Angle" label="Solar Zenith Angle" units="deg" type="scalar" fillvalue="999999" rendering="%.2f" columns="11"/>
		</variables>

	</dataset>

</catalog>