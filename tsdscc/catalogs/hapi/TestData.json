{
    "catalog": {
        "$": {
            "id": "TestData"
        },
        "dataset": [
            {
                "$": {
                    "id": "TestData",
                    "urltemplate": "http://localhost:8999/hapi/data%3Fid=TestData%26time.min=$Y-$m-$d%26time.max=${Y;offset=0}-${m;offset=0}-${d;offset=1}",
                    "delim": ",",
                    "lineregex": "^[0-9][0-9][0-9][0-9]"
                },
                "timeCoverage": [
                    {
                        "Start": [
                            "1970-01-01"
                        ],
                        "End": [
                            "2016-12-31"
                        ],
                        "Cadence": [
                            "PT1S"
                        ]
                    }
                ],
                "variables": [
                    {
                        "variable": [
                            {
                                "$": {
                                    "id": "Time",
                                    "name": "Time",
                                    "units": "UTC",
                                    "fillvalue": null,
                                    "columns": "1"
                                }
                            },
                            {
                                "$": {
                                    "id": "scalar",
                                    "name": "scalar",
                                    "label": "Sine wave with 600 s period",
                                    "units": "m",
                                    "fillvalue": "-1e31",
                                    "columns": "2-2"
                                }
                            },
                            {
                                "$": {
                                    "id": "vector",
                                    "name": "vector",
                                    "units": "m",
                                    "fillvalue": "-1e31",
                                    "columns": "3-5"
                                }
                            },
                            {
                                "$": {
                                    "id": "spectra",
                                    "name": "spectra",
                                    "label": "A time independent 1/f spectra.",
                                    "units": "m",
                                    "fillvalue": "-1e31",
                                    "columns": "6-15"
                                }
                            }
                        ]
                    }
                ]
            }
        ]
    }
}
